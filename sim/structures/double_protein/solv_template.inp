#
# Protein solvated by water and ions
#

# All atoms from different molecules will be at least 2.0 Angstroms apart
# at the solution

tolerance 2.2

# The type of the files will be pdb

filetype pdb

# The name of the output file

output OUTPUT_TO_EDIT

# Add protein

structure PROTEIN_TO_EDIT
  number 1
  #fixed 50. 50. 10. 0. 0. 0.
  fixed 0. 0. 0. 0. 0. 0.
end structure

structure SURFACE_TO_EDIT
  number 1
  #fixed 51.807 51.276 -1. 0. 0. 0.
  fixed 0. 0. 0. 0. 0. 0.
end structure


# Add water molecules

structure ../structs/single_water.pdb
  number 15641
  inside box 0. 0. 0. 172.6900  106.8250   55.2588
  seed -1 #generates random number based on computer clock
end structure

avoid_overlap yes
